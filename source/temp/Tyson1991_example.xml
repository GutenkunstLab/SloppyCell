    <Networks>
        <Network from_file="True" id="base" path="..\temp\base_1497566455.bp" save="True" use_path="False">
            <Set_Constant>
                <var id="cell" is_constant="True" />
                <var id="EmptySet" is_constant="True" />
            </Set_Constant>
            <Set_Optimizable>
