        <Network copy="base" id="perturbed" path="..\temp\perturbed_1497565588.bp" save="True" use_path="True">
            <Set_Initial>
                <var id="k6" value="2.0" />
            </Set_Initial>
            <Set_Optimizable>
                <var id="k6" is_optimizable="False" />
            </Set_Optimizable>
            <Start_Fixed max="1000" min="0" />
