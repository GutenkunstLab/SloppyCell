    <Networks>
        <Network from_file="True" id="base">
            <Set_Constant>
                <var id="cell" is_constant="True" />
                <var id="EmptySet" is_constant="True" />
            </Set_Constant>
            <Set_Optimizable>
                <var id="k5notP" is_optimizable="False" />
